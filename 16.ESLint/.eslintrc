{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "semi": ["error", "never"],
    "no-trailing-spaces": "warn",
    "quotes": ["error", "single"],
    "no-dupe-keys": "error",
    "indent": ["error", 2],
    "comma-dangle": ["error", "never"],
    "no-unused-vars": "warn"
  }
}